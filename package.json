{
  "name": "carrentalhub",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "cd server && npm run dev",
    "client:dev": "cd client && npm run dev",
    "server:start": "cd server && npm start",
    "lint": "eslint . --ext .jsx,.js --fix",
    "test:frontend": "jest tests/frontend --watchAll",
    "test:backend": "jest tests/backend --detectOpenHandles --forceExit",
    "test:all": "concurrently \"npm run test:backend\" \"npm run test:frontend\""
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@eslint/js": "^9.22.0",
    "@testing-library/react": "^16.3.0",
    "babel-jest": "^30.0.5",
    "concurrently": "^9.2.0",
    "eslint": "^9.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.0.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  },
  "dependencies": {
    "stylis": "^4.3.6",
    "stylis-plugin-rtl": "^2.1.1"
  }
}
